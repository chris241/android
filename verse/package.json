{
    "name": "verserealestate",
    "version": "0.0.1",
    "author": "Ionic Framework + JHipster",
    "homepage": "https://www.jhipster.tech",
    "scripts": {
        "ng": "ng",
        "start": "ionic serve",
        "build": "ng build",
        "test": "jest",
        "lint": "ng lint",
        "e2e": "npm run e2e-update && npm run e2e-test",
        "test:watch": "jest --watch",
        "test:ci": "jest --runInBand",
        "test:coverage": "jest --coverage",
        "e2e-test": "protractor ./e2e/protractor.conf.js",
        "e2e-update": "node node_modules/webdriver-manager/bin/webdriver-manager update --gecko false",
        "prettier": "prettier --write \"{,src/**/}*.{js,json,html,md,ts,css,scss,yml}\" --loglevel silent"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "~8.2.14",
        "@angular/core": "~8.2.14",
        "@angular/forms": "~8.2.14",
        "@angular/platform-browser": "~8.2.14",
        "@angular/platform-browser-dynamic": "~8.2.14",
        "@angular/router": "~8.2.14",
        "@fortawesome/angular-fontawesome": "0.6.0",
        "@fortawesome/fontawesome-svg-core": "1.2.27",
        "@fortawesome/free-solid-svg-icons": "5.12.1",
        "@ionic-native/camera": "5.21.6",
        "@ionic-native/core": "^5.0.0",
        "@ionic-native/splash-screen": "^5.0.0",
        "@ionic-native/status-bar": "^5.0.0",
        "@ionic/angular": "^5.0.0",
        "@ng-bootstrap/ng-bootstrap": "5.3.0",
        "@ngx-translate/core": "12.1.1",
        "@ngx-translate/http-loader": "4.0.0",
        "@trixprod/api-verse-real-estate": "file:api/api-verse-real-estate-0.0.4-SNAPSHOT.tgz",
        "cordova-android": "^8.1.0",
        "cordova-ios": "5.1.1",
        "cordova-plugin-camera": "^4.1.0",
        "core-js": "^2.5.4",
        "ng-jhipster": "0.11.5",
        "ngx-webstorage": "4.0.1",
        "rxjs": "~6.5.1",
        "tslib": "^1.9.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.26",
        "@angular/cli": "^8.3.26",
        "@angular/compiler": "~8.2.14",
        "@angular/compiler-cli": "~8.2.14",
        "@angular/language-service": "~8.2.14",
        "@babel/compat-data": "~7.8.0",
        "@ionic/angular-toolkit": "^2.1.1",
        "@ionic/cli": "^6.3.0",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "25.1.3",
        "@types/node": "13.7.4",
        "codelyzer": "^5.0.0",
        "cordova": "^9.0.0",
        "cordova-plugin-device": "^2.0.3",
        "cordova-plugin-ionic-keyboard": "^2.2.0",
        "cordova-plugin-ionic-webview": "^4.1.3",
        "cordova-plugin-splashscreen": "^5.0.3",
        "cordova-plugin-statusbar": "^2.4.3",
        "cordova-plugin-whitelist": "^1.3.4",
        "generator-jhipster": "^6.10.1",
        "generator-jhipster-ionic": "5.1.1",
        "html-loader": "0.5.5",
        "ionic-mocks-jest": "1.3.3",
        "istanbul-instrumenter-loader": "3.0.1",
        "jasmine": "3.5.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "4.2.1",
        "jest": "25.1.0",
        "jest-localstorage-mock": "2.4.0",
        "jest-preset-angular": "^8.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "null-loader": "3.0.0",
        "prettier": "1.19.1",
        "protractor": "5.4.3",
        "ts-loader": "6.2.1",
        "ts-node": "8.6.2",
        "tslint": "~5.15.0",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.4.5",
        "webdriver-manager": "12.1.7"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-camera": {
                "CAMERA_USAGE_DESCRIPTION": "Need camera access to take pictures",
                "PHOTOLIBRARY_USAGE_DESCRIPTION": "Need photo library access to get pictures from there",
                "ANDROID_SUPPORT_V4_VERSION": "27.+"
            },
            "cordova-plugin-whitelist": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-device": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-ionic-webview": {
                "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
            },
            "cordova-plugin-ionic-keyboard": {}
        },
        "platforms": [
            "ios",
            "android"
        ]
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFiles": [
            "jest-localstorage-mock",
            "<rootDir>/src/setupTests.js"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupJest.ts"
        ],
        "testPathIgnorePatterns": [
            "src/test.ts"
        ],
        "testURL": "http://localhost/",
        "transformIgnorePatterns": [
            "node_modules/(?!@ngrx|@ionic-native|@ionic)"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/e2e/"
        ],
        "collectCoverageFrom": [
            "**/*.{ts}",
            "!**/*d.{ts}",
            "!**/*interface.{ts}",
            "!**/*module.{ts}",
            "!**/*main.{ts}",
            "!**/node_modules/**"
        ],
        "moduleFileExtensions": [
            "ts",
            "js",
            "json"
        ],
        "coverageReporters": [
            "json",
            "lcov",
            "text"
        ],
        "coverageDirectory": "./public/coverage"
    },
    "description": "A hipster Ionic project, made with ðŸ’™ by @oktadev!"
}